#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/hub/dist/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/hub/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/hub/dist/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/hub/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules/@huggingface/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/@huggingface+hub@2.2.0/node_modules:/home/fredy/Escritorio/wayuu-spanish-translator/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
